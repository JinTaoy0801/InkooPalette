{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/",
        "destination": "/index.html",
        "type": 301
      },
      {
        "source": "/benchmark1",
        "destination": "/benchmark1/benchmark1.html",
        "type": 302
      },
      {
        "source": "/benchmark2",
        "destination": "/benchmark2/index.html",
        "type": 303
      }
    ]
  }
}
